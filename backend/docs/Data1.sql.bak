),
mx AS (
  SELECT
    m.column1 AS code,
    m.column2 AS name,
    m.column3 AS description,
    m.column4 AS concept_type,
    m.column5 AS debit_code,
    m.column6 AS credit_code,
    ad.id     AS debit_account_id,
    ac.id     AS credit_account_id
  FROM m
  LEFT JOIN acct ad ON ad.code = m.column5
  LEFT JOIN acct ac ON ac.code = m.column6
)
INSERT INTO concepts (
  id,
  code,
  name,
  description,
  concept_type,
  debit,
  debit_account_id,
  credit,
  credit_account_id,
  active,
  user_id,
  created_at,
  updated_at
)
SELECT
  gen_random_uuid(),
  mx.code,
  mx.name,
  mx.description,
  'E',       -- ‚úÖ usar el valor del CTE (o 'E' / 'S' con comillas simples)
  TRUE,
  mx.debit_account_id,
  TRUE,
  mx.credit_account_id,
  TRUE,
  u.uid,
  NOW(),
  NOW()
FROM mx
CROSS JOIN u;



