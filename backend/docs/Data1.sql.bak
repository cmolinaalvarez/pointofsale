INSERT INTO products (
  id,
  name, code, description,
  category_id, price, brand_id, unit_id, cost,
  barcode, image_url,
  active, percent_tax, negative_stock,
  user_id, created_at, updated_at
)
SELECT
  gen_random_uuid() AS id,
  r.name, r.code, r.description,
  c.id            AS category_id,
  r.price,
  b.id            AS brand_id,
  u2.id           AS unit_id,
  r.cost,
  r.barcode, r.image_url,
  r.active, r.percent_tax, r.negative_stock,
  (SELECT uid FROM u) AS user_id,
  COALESCE(r.created_at, NOW()),
  COALESCE(r.updated_at, NOW())
FROM raw r
JOIN cats  c  ON c.n  = r.cat_n
JOIN brs   b  ON b.n  = r.brand_n
JOIN uns   u2 ON u2.n = r.unit_n

-- Idempotencia por code
ON CONFLICT (code) DO UPDATE SET
  name           = EXCLUDED.name,
  description    = EXCLUDED.description,
  category_id    = EXCLUDED.category_id,
  price          = EXCLUDED.price,
  brand_id       = EXCLUDED.brand_id,
  unit_id        = EXCLUDED.unit_id,
  cost           = EXCLUDED.cost,
  barcode        = EXCLUDED.barcode,
  image_url      = EXCLUDED.image_url,
  active         = EXCLUDED.active,
  percent_tax    = EXCLUDED.percent_tax,
  negative_stock = EXCLUDED.negative_stock,
