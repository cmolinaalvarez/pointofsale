INSERT INTO third_parties (
  id,
  name, third_party_type, person_type, contact_name, phone, email, address,
  municipality_id, division_id, country_id,
  nit, active, user_id, created_at, updated_at
)
SELECT
  gen_random_uuid() AS id,
  r.name,
  r.third_party_type::third_party_type_enum AS third_party_type,  -- << CAST a ENUM
  r.person_type::person_type_enum           AS person_type,        -- << CAST a ENUM
  r.contact_name, r.phone, r.email, r.address,
  m.id            AS municipality_id,  -- UUID municipio
  d.id            AS division_id,      -- UUID divisi√≥n (desde divisions)
  co.cid          AS country_id,       -- UUID pa√≠s
  r.nit, r.active,
  u.uid           AS user_id,
  NOW(), NOW()
FROM raw r
JOIN mun_map mm       ON mm.n = r.mun_n
JOIN municipalities m ON m.code::int = mm.muni_code      -- code (varchar) -> int
JOIN divisions d      ON d.code = m.division_code::text  -- asegurar tipos compatibles
CROSS JOIN co
CROSS JOIN u
ON CONFLICT (nit) DO UPDATE SET
  name             = EXCLUDED.name,
  third_party_type = EXCLUDED.third_party_type,
  person_type      = EXCLUDED.person_type,
  contact_name     = EXCLUDED.contact_name,
  phone            = EXCLUDED.phone,
  email            = EXCLUDED.email,
  address          = EXCLUDED.address,
  municipality_id  = EXCLUDED.municipality_id,
  division_id      = EXCLUDED.division_id,
  country_id       = EXCLUDED.country_id,
  active           = EXCLUDED.active,
  user_id          = EXCLUDED.user_id,
  updated_at       = NOW();

