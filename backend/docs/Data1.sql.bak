-- CONCEPTS
WITH u AS (
  SELECT id AS uid FROM users AS u LIMIT 1
),
acct AS (
  SELECT code, id
  FROM accounts
),
m AS (
  -- code, name, description, concept_type, debit_code, credit_code
  VALUES
  ('SALI','Saldos Iniciales','Saldos iniciales de inventario','E','1435','3605'),
  ('COMP','Compra a Proveedor','Ingreso por compras a proveedor','E','1435','2105'),
  ('DEVC','Devolución de Cliente','Cliente devuelve mercancía','E','1435','5105'),
  ('TRAE','Transferencia de Almacén (Ent)','Entrada por traspaso entre bodegas','E','1435','1435'),
  ('AJPI','Ajuste Positivo de Inventario','Ajuste que incrementa existencias','E','1435','5195'),
  ('DONR','Donación Recibida','Ingreso de mercancía por donación','E','1435','4205'),
  ('PROT','Producción Terminada','Ingreso de producto fabricado','E','1435','5195'),
  ('ENTG','Entrada por Garantía','Recepción por garantía (proveedor)','E','1435','2105'),
  ('ENTC','Entrada por Consignación','Ingreso en consignación (propiedad ajena)','E','1435','2705'),
  ('REIF','Recepción por Inventario Físico','Ajuste por toma física (sobrantes)','E','1435','5195'),

  ('VENC','Venta a Cliente','Salida por venta (COGS)','S','5105','1435'),
  ('DEVP','Devolución a Proveedor','Devolución de mercancía a proveedor','S','2105','1435'),
  ('TRAS','Transferencia de Almacén (Sal)','Salida por traspaso entre bodegas','S','1435','1435'),
  ('AJNI','Ajuste Negativo de Inventario','Ajuste que reduce existencias','S','5195','1435'),
  ('MUEG','Muestras Gratis','Salida por muestras','S','6140','1435'),
  ('DONE','Donación Entregada','Salida por donación','S','6140','1435'),
  ('CONI','Consumo Interno','Salida por consumo interno','S','5195','1435'),
  ('PERD','Pérdida por Deterioro','Merma por deterioro','S','5195','1435'),
  ('ROPM','Robo o Merma','Pérdida por robo o merma','S','5195','1435'),
  ('SALG','Salida por Garantía','Envío a garantía','S','5195','1435'),
  ('PROD','Producto Dañado','Salida por producto no apto','S','5195','1435'),
  ('PROV','Producto Vencido','Salida por vencimiento','S','5195','1435'),
  ('PEDC','Pedido Cliente','Salida por preparación de pedido','S','5195','1435')
),
mx AS (
  SELECT
    m.column1 AS code,
    m.column2 AS name,
    m.column3 AS description,
    m.column4 AS concept_type,
    m.column5 AS debit_code,
    m.column6 AS credit_code,
    ad.id     AS debit_account_id,
    ac.id     AS credit_account_id
  FROM m
  LEFT JOIN acct ad ON ad.code = m.column5
  LEFT JOIN acct ac ON ac.code = m.column6
)
INSERT INTO concepts (
  id,
  code,
  name,
  description,
  concept_type,
  debit,
  debit_account_id,
  credit,
  credit_account_id,
  active,
  user_id,
  created_at,
  updated_at
)
SELECT
  gen_random_uuid(),
  mx.code,
  mx.name,
  mx.description,
  'E',       -- ✅ usar el valor del CTE (o 'E' / 'S' con comillas simples)
  TRUE,
  mx.debit_account_id,
  TRUE,
  mx.credit_account_id,
  TRUE,
  u.uid,
  NOW(),
  NOW()
FROM mx
CROSS JOIN u;



