-- üßæ Inserci√≥n final usando el mapeo dd (division_code -> division_id UUID)
INSERT INTO municipalities (
  id,
  name,
  code,
  division_id,     -- UUID de divisions.id
  division_code,   -- c√≥digo DANE del dpto (int)
  user_id,
  active,
  created_at,
  updated_at
)
SELECT
  gen_random_uuid()                              AS id,
  r.name,
  r.code,
  dd.division_id,                                -- <- GUID correcto desde divisions
  dd.division_code,
  (SELECT uid FROM u)                            AS user_id,
  'True',
  
  COALESCE(r.created_at, NOW())                  AS created_at,
  COALESCE(r.updated_at, NOW())                  AS updated_at
FROM raw r
JOIN dd ON dd.division_code = r.division_code;

--PRODUCT
WITH u AS (
   SELECT id as uid FROM users as u LIMIT 1 
),

-- üîñ Mapeo estable de n√∫meros (legacy) ‚Üí categor√≠as por CODE
cat_map(n, code) AS (
  VALUES
  (1,'BEB'), (2,'ALI'), (3,'SNA'), (4,'LAC'), (5,'FRU')
),
cats AS (
  SELECT cm.n, c.id
  FROM cat_map cm
  JOIN categories c ON c.code = cm.code
),

-- üè∑Ô∏è Mapeo estable de n√∫meros (legacy) ‚Üí marcas por NAME
brand_map(n, name) AS (
  VALUES
  (1,'Samsung'), (2,'LG'), (3,'Apple'), (4,'Sony'), (5,'Lenovo')
),
brs AS (
  SELECT bm.n, b.id
  FROM brand_map bm
  JOIN brands b ON b.name = bm.name
),

-- üìè Mapeo estable de n√∫meros (legacy) ‚Üí unidades por SYMBOL
unit_map(n, symbol) AS (
  VALUES
  (1,'un'), (2,'kg'), (3,'g')
),
uns AS (
  SELECT um.n, u.id
  FROM unit_map um
  JOIN units u ON u.symbol = um.symbol
),

-- üì¶ Tu data original (incluye el user_id num√©rico como legacy_user, lo ignoramos)
